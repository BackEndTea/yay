--TEST--
Proof of concept literal object syntax
--FILE--
<?php

macro {

    {
        ·ls
        (
            ·chain
            (
                ·word()·key
                ,
                ·token(':')
                ,
                ·parentheses()·value
            )
            ,
            ·token(',')
        )
        ·properties
    }

} >> {

    ((object)[·properties ··· { @·key => (·value), }])

}

$obj = { key : ("value"), other_key : (1 + 1), another_key : (new \stdclass) };

var_dump($obj);

?>
--EXPECTF--
<?php

$obj = ((object)[@key => ("value"), @other_key => (1 + 1), @another_key => (new \stdclass), ]);

var_dump($obj);

?>
